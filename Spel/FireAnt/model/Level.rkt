(define (new-level player file)
  (define (init-maze file)
    (let fill-maze (maze ()<++>)<++>)<++>)

  (let ((maze (init-maze file))
        (ant player))

    (define (start)
      (let ((levels (directory-list "level")))
        (display levels)))

    (define (dispatch-game cmd)
      (cond ((eq? cmd 'start) start)))

    dispatch-game))
